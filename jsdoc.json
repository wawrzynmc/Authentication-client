{
    "source": {
        "include": ["./src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/better-docs/category",
        "node_modules/better-docs/component"
    ],
    "opts": {
        "encoding": "utf8",
        "readme": "./readme.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": false,
        "template": "node_modules/better-docs"
    }
}